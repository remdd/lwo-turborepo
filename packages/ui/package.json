{
  "name": "@lwo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    "./components": "./src/components/index.tsx",
    "./layouts": "./src/layouts/index.tsx",
    "./fonts": "./src/fonts/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "check-types": "tsc --noEmit",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "lint": "eslint src --max-warnings 0"
  },
  "peerDependencies": {
    "react": "^19"
  },
  "devDependencies": {
    "@lwo/eslint-config": "*",
    "@lwo/tailwind-config": "*",
    "@lwo/typescript-config": "*",
    "@types/react": "^19.0.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "framer-motion": "^12.6.0",
    "next": "^15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
