{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "web.content-row",
        "web.faq-collection",
        "web.content-blocks"
      ],
      "required": true
    },
    "article_tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article-tag.article-tag"
    }
  }
}
